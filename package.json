  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-app-rewired": "^2.1.8"
  },
  "jest": {
    "moduleNameMapper": {
      "^@components(/?)(.*?)$": [
        "shared/components$1$2"
      ],
      "^@configs(/?)(.*?)$": [
        "shared/configs$1$2"
      ],
      "^@layouts(/?)(.*?)$": [
        "shared/layouts$1$2"
      ],
      "^@utils(/?)(.*?)$": [
        "shared/utils$1$2"
      ],
      "^@pages(/?)(.*?)$": [
        "pages$1$2"
      ],
      "^@images(/?)(.*?)$": [
        "assets/images$1$2"
      ],
      "^@sass(/?)(.*?)$": [
        "assets/scss$1$2"
      ]
    }
